program ->  function
program ->  function program
function ->  FUNC ID LPAREN args RPAREN locals BEGIN statement END
args ->  var COMMA args
args ->  var
args -> 
locals ->  varass SEMICOLON locals
locals -> 
varass ->  var
varass ->  assignation
var ->  ID COLON TYPE
assignation ->  ID COLONEQUAL INTEGER
assignation ->  ID COLONEQUAL FLOAT
assignation ->  ID COLONEQUAL STRING
assignation ->  ID COLONEQUAL BOOLEAN
assignation ->  ID COLONEQUAL ID
assignation ->  ID COLONEQUAL expression
statement ->  controlstructure statement
statement ->  instruction statement
statement -> 
controlstructure ->  cif
controlstructure ->  cwhile
cwhile ->  WHILE LPAREN conditional RPAREN DO BEGIN statement END SEMICOLON
cwhile ->  WHILE conditional DO BEGIN statement END SEMICOLON
cwhile ->  WHILE LPAREN conditional RPAREN DO instruction SEMICOLON
cwhile ->  WHILE conditional DO instruction SEMICOLON
cif ->  IF LPAREN conditional RPAREN THEN BEGIN statement END SEMICOLON
cif ->  IF conditional THEN BEGIN statement END SEMICOLON
cif ->  IF LPAREN conditional RPAREN THEN instruction
cif ->  IF conditional THEN instruction
conditional ->  expression GREATER expression
conditional ->  expression LESS expression
conditional ->  expression GREATEREQUAL expression
conditional ->  expression LESSEQUAL expression
conditional ->  expression DISTINT expression
conditional ->  expression NOT expression
conditional ->  expression OR expression
conditional ->  expression AND expression
conditional ->  NOT expression
conditional ->  LPARENT expression RPARENT
conditional ->  BOOLEAN
expression ->  expression PLUS prod
expression ->  expression MINUS Pprod
expression ->  prod
prod ->  prod MULTIPLY term
prod ->  prod DIVIDE term
prod ->  term
term ->  ID
term ->  FLOAT
term ->  INTEGER
term ->  LPAREN expression RPAREN
term ->  LPAREN ID RPAREN
term ->  LPAREN ID RPAREN
instruction ->  return
instruction ->  print
instruction ->  assignation
instruction ->  SKIP
instruction ->  read
instruction ->  write
instruction ->  ID LPAREN args RPAREN
instruction ->  BREAK
return ->  RETURN INTEGER
return ->  RETURN FLOAT
return ->  RETURN STRING
return ->  RETURN ID
return ->  RETURN expression
print ->  PRINT LPAREN STRING RPAREN
print ->  PRINT LPAREN FLOAT RPAREN
print ->  PRINT LPAREN INTEGER RPAREN
print ->  PRINT LPAREN BOOLEAN RPAREN
read ->  READ LPAREN expression RPAREN
write ->  WRITE LPAREN expression RPAREN
